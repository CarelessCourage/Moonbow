{
  "name": "moonbow",
  "version": "0.1.0",
  "description": "Vue component for adding GLSL to images",
  "author": "Samuel M. Bednarz<https://github.com/CarelessCourage>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CarelessCourage/Moonbow"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/moonbow.umd.js",
  "module": "./dist/moonbow.es.js",
  "exports": {
    ".": {
      "import": "./dist/moonbow.es.js",
      "require": "./dist/moonbow.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "sass": "^1.56.1",
    "three": "^0.146.0",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@types/node": "^16.11.68",
    "@types/three": "^0.146.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/tsconfig": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vite-plugin-glsl": "^0.5.4",
    "vue-tsc": "^1.0.8"
  }
}
